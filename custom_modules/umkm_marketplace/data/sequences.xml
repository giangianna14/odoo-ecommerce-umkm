<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for UMKM Orders -->
        <record id="sequence_umkm_order" model="ir.sequence">
            <field name="name">UMKM Order</field>
            <field name="code">umkm.order</field>
            <field name="prefix">ORD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for UMKM Vendor Orders -->
        <record id="sequence_umkm_vendor_order" model="ir.sequence">
            <field name="name">UMKM Vendor Order</field>
            <field name="code">umkm.vendor.order</field>
            <field name="prefix">VND</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for UMKM Commissions -->
        <record id="sequence_umkm_commission" model="ir.sequence">
            <field name="name">UMKM Commission</field>
            <field name="code">umkm.commission</field>
            <field name="prefix">COM</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for UMKM Vendors -->
        <record id="sequence_umkm_vendor" model="ir.sequence">
            <field name="name">UMKM Vendor</field>
            <field name="code">umkm.vendor</field>
            <field name="prefix">VEN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
